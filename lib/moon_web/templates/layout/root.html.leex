<% asset_import(@conn, "js/themes/bitcasino-light.js") %>
<% asset_import(@conn, "js/themes/bombay-club.js") %>
<% asset_import(@conn, "js/themes/btcxe.js") %>
<% asset_import(@conn, "js/themes/hub88.js") %>
<% asset_import(@conn, "js/themes/livecasino-dark.js") %>
<% asset_import(@conn, "js/themes/livecasino-light.js") %>
